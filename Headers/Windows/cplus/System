// ALL in Base C+
// Windows

function clear(var) {
  var = "";
}

function log(arg: void) {
  *GLOBAL string console;
  console.append(arg);
  *GLOBAL *CPLUS CLI_BASE = console;
}

function append(arg: void) {
  *GLOBAL += arg;
}

function print(arg: void) {
  for i in arg {
    *GLOBAL string cachelim;
    cachelim.append(i);
  }
  log(cachelim);
  clear(cachelim);
}

function ghostprint(arg: void) {
  log(arg);
  arg = "";
}
